{
  "name": "Murora Light",
  "type": "light",
  "colors": {
    "editor.background": "#FAFAFA",
    "editor.foreground": "#2E3440",
    "activityBarBadge.background": "#5E81AC",
    "sideBarTitle.foreground": "#2E3440",
    "titleBar.activeBackground": "#ECEFF4",
    "titleBar.activeForeground": "#2E3440",
    "statusBar.background": "#D8DEE9",
    "statusBar.foreground": "#2E3440",
    "activityBar.background": "#E5E9F0",
    "activityBar.foreground": "#2E3440",
    "sideBar.background": "#ECEFF4",
    "list.activeSelectionBackground": "#D8DEE9",
    "list.inactiveSelectionBackground": "#E5E9F0",
    "tab.activeBackground": "#E5E9F0",
    "tab.inactiveBackground": "#FAFAFA",
    "tab.activeForeground": "#2E3440",
    "tab.inactiveForeground": "#4C566A",
    "editorGroupHeader.tabsBackground": "#FAFAFA",
    "editorLineNumber.foreground": "#4C566A",
    "editor.selectionBackground": "#D8DEE9",
    "editor.wordHighlightBackground": "#E5E9F0",
    "editor.lineHighlightBackground": "#E5E9F0",
    "editorCursor.foreground": "#2E3440"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#4C566A"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#2E3440"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#2E3440"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#BF616A"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#C678DD"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#5E81AC"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "meta.tag.sgml"],
      "settings": {
        "foreground": "#BF616A"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#6F96FF"
      }
    },
    {
      "name": "Class, Support",
      "scope": ["entity.name", "support.type"],
      "settings": {
        "foreground": "#C678DD"
      }
    },
    {
      "name": "Number, Constant, Function Argument",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#D08770"
      }
    },
    {
      "name": "String, Symbols",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key"
      ],
      "settings": {
        "foreground": "#18702A"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#8FBCBB"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.scss support.type.property-name"
      ],
      "settings": {
        "foreground": "#8FBCBB"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#D08770"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#18702A"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#BF616A"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#5E81AC"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#5E81AC"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "foreground": "#5E81AC"
      }
    },
    {
      "name": "Namespace and Use Keywords",
      "scope": [
        "keyword.other.namespace",
        "keyword.control.import",
        "entity.name.namespace"
      ],
      "settings": {
        "foreground": "#C678DD"
      }
    }
  ]
}
