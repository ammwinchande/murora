{
  "name": "Murora Light",
  "type": "light",
  "colors": {
    "editor.background": "#FFFFFF",
    "editor.foreground": "#1A202C",
    "activityBarBadge.background": "#4C6998",
    "sideBarTitle.foreground": "#1A202C",
    "titleBar.activeBackground": "#F0F3F9",
    "titleBar.activeForeground": "#1A202C",
    "statusBar.background": "#E2E8F0",
    "statusBar.foreground": "#1A202C",
    "activityBar.background": "#F0F3F9",
    "activityBar.foreground": "#1A202C",
    "sideBar.background": "#F0F3F9",
    "list.inactiveSelectionBackground": "#EDF2F7",
    "tab.activeBackground": "#E2E8F0",
    "tab.inactiveBackground": "#F0F3F9",
    "tab.activeForeground": "#1A202C",
    "tab.inactiveForeground": "#3D4852",
    "editorGroupHeader.tabsBackground": "#F0F3F9",
    "editorLineNumber.foreground": "#3D4852",
    "editor.selectionBackground": "#E2E8F0",
    "editor.lineHighlightBackground": "#EDF2F7",
    "editorCursor.foreground": "#1A202C"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#64748B",
        "fontStyle": ""
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#1E293B",
        "fontStyle": ""
      }
    },
    {
      "name": "Colors",
      "scope": [
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#2D3748",
        "fontStyle": ""
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#AB2F3C",
        "fontStyle": ""
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#7C3B7A",
        "fontStyle": ""
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#4A6997",
        "fontStyle": ""
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#AB2F3C",
        "fontStyle": ""
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#4A6997",
        "fontStyle": ""
      }
    },
    {
      "name": "Block Level Variables",
      "scope": [
        "meta.block variable.other"
      ],
      "settings": {
        "foreground": "#1E293B",
        "fontStyle": ""
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": [
        "support.other.variable",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#AB2F3C",
        "fontStyle": ""
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#A23D1A",
        "fontStyle": ""
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#3A6948",
        "fontStyle": ""
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types",
        "meta.import meta.block variable.other.readwrite",
        "meta.import meta.block variable.other.readwrite.alias"
      ],
      "settings": {
        "foreground": "#A67611",
        "fontStyle": "",
      }
    },
    {
      "name": "Entity Types",
      "scope": [
        "support.type"
      ],
      "settings": {
        "foreground": "#2E6C6A",
        "fontStyle": ""
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#2E6C6A",
        "fontStyle": ""
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#AB2F3C",
        "fontStyle": ""
      }
    },
    {
      "name": "Language methods",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "fontStyle": "italic ",
        "foreground": "#AB2F3C"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": [
        "entity.name.method.js"
      ],
      "settings": {
        "fontStyle": "italic ",
        "foreground": "#4A6997"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#4A6997",
        "fontStyle": ""
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#A23D1A",
        "fontStyle": ""
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic ",
        "foreground": "#A67611"
      }
    },
    {
      "name": "Inserted",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#3A6948",
        "fontStyle": ""
      }
    },
    {
      "name": "Deleted",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#AB2F3C",
        "fontStyle": ""
      }
    },
    {
      "name": "Changed",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#7C3B7A",
        "fontStyle": ""
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#4A6997",
        "fontStyle": ""
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#4A6997",
        "fontStyle": ""
      }
    },
    {
      "name": "URL",
      "scope": [
        "*url*",
        "*link*",
        "*uri*"
      ],
      "settings": {
        "fontStyle": "underline "
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic ",
        "foreground": "#4A6997"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#4A6997",
        "fontStyle": ""
      }
    },
    {
      "name": "Markdown - Plain",
      "scope": [
        "text.html.markdown",
        "punctuation.definition.list_item.markdown"
      ],
      "settings": {
        "foreground": "#1A202C",
        "fontStyle": ""
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": [
        "markdown.heading",
        "markup.heading | markup.heading entity.name",
        "markup.heading.markdown punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#A67611",
        "fontStyle": ""
      }
    },
    {
      "name": "Markdown - Italic",
      "scope": [
        "markup.italic",
        "markup.italic string"
      ],
      "settings": {
        "fontStyle": "italic ",
        "foreground": "#1A202C"
      }
    },
    {
      "name": "Markdown - ",
      "scope": [
        "markup.",
        "markup. string"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#1A202C"
      }
    },
    {
      "name": "Markdown - Link",
      "scope": [
        "string.other.link.title.markdown"
      ],
      "settings": {
        "foreground": "#4A6997",
        "fontStyle": ""
      }
    }
  ],
  "editor.fontWeight": "600"
}
