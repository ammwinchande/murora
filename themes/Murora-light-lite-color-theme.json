{
    "name": "Murora Light Lite",
    "type": "light",
    "colors": {
        "editor.background": "#ffffff",
        "editor.foreground": "#222222",
        "editor.lineHighlightBackground": "#f7f7f7",
        "editor.selectionBackground": "#dddddd",
        "editor.inactiveSelectionBackground": "#eeeeee80",
        "editor.findMatchBackground": "#fcbc05",
        "editor.findMatchHighlightBackground": "#fcbc0550",
        "editorCursor.foreground": "#222222",
        "editorLineNumber.foreground": "#555555",
        "editorLineNumber.activeForeground": "#222222",
        "editorIndentGuide.background1": "#f0f0f0",
        "editorIndentGuide.activeBackground1": "#dddddd",
        "editorWhitespace.foreground": "#f0f0f0",
        "editorRuler.foreground": "#f0f0f0",
        "editorError.foreground": "#e84135",
        "editorWarning.foreground": "#ea4334",
        "editorInfo.foreground": "#fcbc05",
        "sideBar.background": "#ffffff",
        "sideBar.foreground": "#222222",
        "sideBarSectionHeader.background": "#f7f7f7",
        "activityBar.background": "#ffffff",
        "activityBar.foreground": "#222222",
        "statusBar.background": "#ffffff",
        "statusBar.foreground": "#222222",
        "titleBar.activeBackground": "#ffffff",
        "titleBar.activeForeground": "#222222",
        "panel.background": "#fefefe",
        "panel.border": "#f0f0f0",
        "terminal.background": "#fefefe",
        "terminal.foreground": "#333333",
        "terminal.ansiBlack": "#333333",
        "terminal.ansiRed": "#e84135",
        "terminal.ansiGreen": "#34a853",
        "terminal.ansiYellow": "#fcbc05",
        "terminal.ansiBlue": "#4285f4",
        "terminal.ansiMagenta": "#ea4334",
        "terminal.ansiCyan": "#4adc6e",
        "terminal.ansiWhite": "#222222",
        "terminal.ansiBrightBlack": "#555555",
        "terminal.ansiBrightRed": "#ff3e25",
        "terminal.ansiBrightGreen": "#4adc6e",
        "terminal.ansiBrightYellow": "#fcde00",
        "terminal.ansiBrightBlue": "#4fafff",
        "terminal.ansiBrightMagenta": "#ea4334",
        "terminal.ansiBrightCyan": "#4adc6e",
        "terminal.ansiBrightWhite": "#222222",
        "tab.activeBackground": "#ffffff",
        "tab.activeForeground": "#222222",
        "tab.inactiveBackground": "#f7f7f7",
        "tab.inactiveForeground": "#555555",
        "breadcrumb.background": "#ffffff",
        "breadcrumb.foreground": "#555555",
        "breadcrumb.activeSelectionForeground": "#222222"
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#454545",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Documentation Comment",
            "scope": [
                "comment.block.documentation",
                "comment.line.documentation"
            ],
            "settings": {
                "foreground": "#777777",
                "fontStyle": "italic"
            }
        },
        {
            "name": "String",
            "scope": [
                "string",
                "string.quoted"
            ],
            "settings": {
                "foreground": "#222222"
            }
        },
        {
            "name": "Number",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#222222"
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#4285f4"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#555555"
            }
        },
        {
            "name": "Function Declaration",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#34a853"
            }
        },
        {
            "name": "Function Call",
            "scope": [
                "meta.function-call",
                "variable.function"
            ],
            "settings": {
                "foreground": "#33a652"
            }
        },
        {
            "name": "Class Name",
            "scope": [
                "entity.name.class",
                "entity.name.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#ea4334"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable",
                "variable.other"
            ],
            "settings": {
                "foreground": "#ea4334"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "constant",
                "constant.language",
                "support.constant"
            ],
            "settings": {
                "foreground": "#34a853",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Built-in / Predefined",
            "scope": [
                "support.type",
                "support.variable",
                "variable.language"
            ],
            "settings": {
                "foreground": "#34a853",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Tag",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#4285f4"
            }
        },
        {
            "name": "Attribute",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#34a853"
            }
        },
        {
            "name": "JSON Property",
            "scope": [
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#4285f4"
            }
        },
        {
            "name": "JSON Punctuation",
            "scope": [
                "punctuation.support.type.property-name.begin.json",
                "punctuation.support.type.property-name.end.json",
                "punctuation.separator.dictionary.key-value.json",
                "punctuation.separator.dictionary.pair.json",
                "punctuation.definition.dictionary.begin.json",
                "punctuation.definition.dictionary.end.json",
                "punctuation.definition.array.begin.json",
                "punctuation.definition.array.end.json"
            ],
            "settings": {
                "foreground": "#34a853"
            }
        },
        {
            "name": "PHP Variable",
            "scope": [
                "variable.other.php",
                "punctuation.definition.variable.php"
            ],
            "settings": {
                "foreground": "#ea4334"
            }
        },
        {
            "name": "PHP Instance Field",
            "scope": [
                "variable.other.property.php"
            ],
            "settings": {
                "foreground": "#34a853"
            }
        },
        {
            "name": "PHP Type Hint",
            "scope": [
                "storage.type.php",
                "keyword.other.type.php"
            ],
            "settings": {
                "foreground": "#ea4334"
            }
        },
        {
            "name": "JavaScript Global Function",
            "scope": [
                "support.function.js",
                "support.function.dom.js"
            ],
            "settings": {
                "foreground": "#8e00"
            }
        },
        {
            "name": "JavaScript Instance Member",
            "scope": [
                "variable.other.object.property.js"
            ],
            "settings": {
                "foreground": "#8e00"
            }
        },
        {
            "name": "Markdown Code",
            "scope": [
                "markup.inline.raw.markdown",
                "markup.fenced_code.block.markdown"
            ],
            "settings": {
                "foreground": "#424242",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown Code Fence",
            "scope": [
                "punctuation.definition.markdown"
            ],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "YAML Key",
            "scope": [
                "entity.name.tag.yaml"
            ],
            "settings": {
                "foreground": "#34a853"
            }
        },
        {
            "name": "Deprecated",
            "scope": [
                "invalid.deprecated"
            ],
            "settings": {
                "foreground": "#ea4334",
                "fontStyle": "strikethrough"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid"
            ],
            "settings": {
                "foreground": "#e84135"
            }
        }
    ]
}
