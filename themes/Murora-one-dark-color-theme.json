{
    "name": "Murora One Dark",
    "type": "dark",
    "colors": {
        "editor.background": "#1E2128",
        "editor.foreground": "#D8DEE9",
        "activityBarBadge.background": "#88C0D0",
        "sideBarTitle.foreground": "#D8DEE9",
        "titleBar.activeBackground": "#1E2128",
        "titleBar.activeForeground": "#D8DEE9",
        "statusBar.background": "#3B4252",
        "statusBar.foreground": "#D8DEE9",
        "activityBar.background": "#1E2128",
        "activityBar.foreground": "#D8DEE9",
        "sideBar.background": "#1E2128",
        "list.activeSelectionBackground": "#434C5E",
        "list.inactiveSelectionBackground": "#3B4252",
        "tab.activeBackground": "#3B4252",
        "tab.inactiveBackground": "#1E2128",
        "tab.activeForeground": "#D8DEE9",
        "tab.inactiveForeground": "#81A1C1",
        "editorGroupHeader.tabsBackground": "#1E2128",
        "editorLineNumber.foreground": "#495162",
        "editorLineNumber.activeForeground": "#abb2bf",
        "editor.selectionBackground": "#434C5E",
        "editor.wordHighlightBackground": "#d2e0ff2f",
        "editor.wordHighlightBorder": "#7f848e",
        "editor.wordHighlightStrongBackground": "#abb2bf26",
        "editor.wordHighlightStrongBorder": "#7f848e",
        "editor.lineHighlightBackground": "#3B4252",
        "editorCursor.foreground": "#D8DEE9",
        "editorBracketHighlight.foreground1": "#D8DEE9",
        "editorBracketHighlight.foreground2": "#D8DEE9",
        "editorBracketHighlight.foreground3": "#56b6c2",
        "editorBracketMatch.background": "#515a6b",
        "editorBracketMatch.border": "#515a6b",
        "editor.findMatchBackground": "#d19a6644",
        "editor.findMatchBorder": "#ffffff5a",
        "editor.findMatchHighlightBackground": "#ffffff22",
        "editorGutter.addedBackground": "#109868",
        "editorGutter.deletedBackground": "#9A353D",
        "editorGutter.modifiedBackground": "#948B60",
        "editorIndentGuide.activeBackground1": "#c8c8c859",
        "editorIndentGuide.background1": "#3b4048",
        "minimapGutter.addedBackground": "#109868",
        "minimapGutter.deletedBackground": "#9A353D",
        "minimapGutter.modifiedBackground": "#948B60",
        "editorOverviewRuler.addedBackground": "#109868",
        "editorOverviewRuler.deletedBackground": "#9A353D",
        "editorOverviewRuler.modifiedBackground": "#948B60",
        "gitDecoration.ignoredResourceForeground": "#636b78"
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#5C6370",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "string constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#D8DEE9"
            }
        },
        {
            "name": "Colors",
            "scope": [
                "constant.other.color"
            ],
            "settings": {
                "foreground": "#ECEFF4"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#BF616A"
            }
        },
        {
            "name": "Keyword, Storage",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#C678DD"
            }
        },
        {
            "name": "Operator, Misc",
            "scope": [
                "keyword.control",
                "constant.other.color",
                "punctuation",
                "meta.tag",
                "punctuation.definition.tag",
                "punctuation.separator.inheritance.php",
                "punctuation.definition.tag.html",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html",
                "punctuation.section.embedded",
                "keyword.other.template",
                "keyword.other.substitution"
            ],
            "settings": {
                "foreground": "#81A1C1"
            }
        },
        {
            "name": "Tag",
            "scope": [
                "entity.name.tag",
                "meta.tag.sgml",
                "markup.deleted.git_gutter"
            ],
            "settings": {
                "foreground": "#BF616A"
            }
        },
        {
            "name": "Function, Special Method",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "variable.function",
                "support.function",
                "keyword.other.special-method"
            ],
            "settings": {
                "foreground": "#6F96FF"
            }
        },
        {
            "name": "Block Level Variables",
            "scope": [
                "meta.block variable.other"
            ],
            "settings": {
                "foreground": "#D8DEE9"
            }
        },
        {
            "name": "Other Variable, String Link",
            "scope": [
                "support.other.variable",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#BF616A"
            }
        },
        {
            "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant",
                "constant.character",
                "constant.escape",
                "variable.parameter",
                "keyword.other.unit"
            ],
            "settings": {
                "foreground": "#D08770"
            }
        },
        {
            "name": "String, Symbols, Inherited Class, Markup Heading",
            "scope": [
                "string",
                "constant.other.symbol",
                "constant.other.key",
                "entity.other.inherited-class",
                "markup.heading",
                "markup.inserted.git_gutter",
                "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
            ],
            "settings": {
                "foreground": "#9ABE6A"
            }
        },
        {
            "name": "Class, Support",
            "scope": [
                "entity.name",
                "support.other.namespace.use.php"
            ],
            "settings": {
                "foreground": "#C678DD"
            }
        },
        {
            "name": "Class, Support",
            "scope": [
                "entity.name",
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#EBCB8B"
            }
        },
        {
            "name": "Entity Types",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#8FBCBB"
            }
        },
        {
            "name": "CSS Class and Support",
            "scope": [
                "source.css support.type.property-name",
                "source.sass support.type.property-name",
                "source.scss support.type.property-name",
                "source.less support.type.property-name",
                "source.stylus support.type.property-name",
                "source.postcss support.type.property-name"
            ],
            "settings": {
                "foreground": "#8FBCBB"
            }
        },
        {
            "name": "Sub-methods",
            "scope": [
                "entity.name.module.js",
                "variable.import.parameter.js",
                "variable.other.class.js"
            ],
            "settings": {
                "foreground": "#BF616A"
            }
        },
        {
            "name": "Language methods",
            "scope": [
                "variable.language"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#BF616A"
            }
        },
        {
            "name": "entity.name.method.js",
            "scope": [
                "entity.name.method.js"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#88C0D0"
            }
        },
        {
            "name": "meta.method.js",
            "scope": [
                "meta.class-method.js entity.name.function.js",
                "variable.function.constructor"
            ],
            "settings": {
                "foreground": "#88C0D0"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#D08770"
            }
        },
        {
            "name": "HTML Attributes",
            "scope": [
                "text.html.basic entity.other.attribute-name.html",
                "text.html.basic entity.other.attribute-name"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#EBCB8B"
            }
        },
        {
            "name": "Inserted",
            "scope": [
                "markup.inserted"
            ],
            "settings": {
                "foreground": "#9ABE6A"
            }
        },
        {
            "name": "Deleted",
            "scope": [
                "markup.deleted"
            ],
            "settings": {
                "foreground": "#BF616A"
            }
        },
        {
            "name": "Changed",
            "scope": [
                "markup.changed"
            ],
            "settings": {
                "foreground": "#B48EAD"
            }
        },
        {
            "name": "Regular Expressions",
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#88C0D0"
            }
        },
        {
            "name": "Escape Characters",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#88C0D0"
            }
        },
        {
            "name": "URL",
            "scope": [
                "*url*",
                "*link*",
                "*uri*"
            ],
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "Decorators",
            "scope": [
                "tag.decorator.js entity.name.tag.js",
                "tag.decorator.js punctuation.definition.tag.js"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#88C0D0"
            }
        },
        {
            "name": "JSON Key - Level 0",
            "scope": [
                "source.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#88C0D0"
            }
        },
        {
            "name": "Markdown - Plain",
            "scope": [
                "text.html.markdown",
                "punctuation.definition.list_item.markdown"
            ],
            "settings": {
                "foreground": "#D8DEE9"
            }
        },
        {
            "name": "Markdown - Heading",
            "scope": [
                "markdown.heading",
                "markup.heading | markup.heading entity.name",
                "markup.heading.markdown punctuation.definition.heading.markdown"
            ],
            "settings": {
                "foreground": "#EBCB8B"
            }
        },
        {
            "name": "Markdown - Italic",
            "scope": [
                "markup.italic",
                "markup.italic string"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#D8DEE9"
            }
        },
        {
            "name": "Markdown - Bold",
            "scope": [
                "markup.bold",
                "markup.bold string"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#D8DEE9"
            }
        },
        {
            "name": "Markdown - Link",
            "scope": [
                "string.other.link.title.markdown"
            ],
            "settings": {
                "foreground": "#88C0D0"
            }
        },
        {
            "name": "Namespace and Use Keywords",
            "scope": [
                "keyword.other.namespace",
                "keyword.control.import",
                "entity.name.namespace",
                "keyword.control.return",
                "keyword.other.use"
            ],
            "settings": {
                "foreground": "#C678DD"
            }
        },
        {
            "name": "Namespace Imports",
            "scope": [
                "keyword.other.namespace"
            ],
            "settings": {
                "foreground": "#C678DD"
            }
        },
        {
            "name": "New Keywords",
            "scope": [
                "keyword.other.new",
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#78D5FE"
            }
        }
    ]
}
